{
  "name": "laneshare",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter @laneshare/web dev",
    "build": "pnpm --filter @laneshare/web build",
    "start": "pnpm --filter @laneshare/web start",
    "lint": "pnpm --filter @laneshare/web lint",
    "test": "pnpm -r --if-present test",
    "db:migrate": "cd supabase && supabase db push",
    "db:reset": "cd supabase && supabase db reset",
    "worker:dev": "pnpm --filter @laneshare/worker dev"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
